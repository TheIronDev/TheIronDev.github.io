<!DOCTYPE html>
<html>
<head>
	<title><%= @document.title %> | The Iron Developer's Home</title>
	<meta name="keywords" content="Tyler Stark, The Iron Developer" />
	<meta charset="UTF-8" />
	<meta name="author" content="Tyler Stark">
	<meta name="viewport" content="intial-scale=1.0, width=device-width, user-scalable=no" />
	<%- @getBlock("meta").toHTML() %>
	<link href='http://fonts.googleapis.com/css?family=Titillium+Web' rel='stylesheet' type='text/css'>
	<%- @getBlock("styles").add(["/styles/style.css"]).add(["/styles/highlightjs-github.css"]).toHTML() %>
	<link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" />
</head>
<body>
<div class="container">
	<div class="container-inner <%= @document.wrapperClass %>">
		<h1>
			<a href="/">TheIronDeveloper</a><% if @document.title : %>: <%= @document.title %><% end %>
		</h1>

		<div class="content">
			<%- @content %>
		</div>
	</div>
</div>
<%- @getBlock("scripts").toHTML() %>
<script data-main="/scripts/config" src="/scripts/lib/require.js"></script>
</body>
</html>
